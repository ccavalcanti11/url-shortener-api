<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!--
        This file contains suppressions for known false positives in OWASP Dependency Check.
        Add suppressions here for dependencies that have been verified as safe.
    -->

    <!-- Example suppression for Spring Boot related false positives -->
    <suppress>
        <notes><![CDATA[
        Spring Boot starter dependencies are regularly updated and maintained by VMware.
        These are not direct security vulnerabilities in our application.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/.*$</packageUrl>
        <cpe>cpe:/a:pivotal:spring_framework</cpe>
    </suppress>

    <!-- Example suppression for Lombok (build-time only dependency) -->
    <suppress>
        <notes><![CDATA[
        Lombok is only used at compile time and not included in the final artifact.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.projectlombok/lombok.*$</packageUrl>
    </suppress>
</suppressions>

